{
  "name": "@roughapp/slack-bot",
  "private": true,
  "version": "0.12.0",
  "packageManager": "pnpm@9.12.3",
  "type": "module",
  "main": "./src/index.ts",
  "author": {
    "name": "George Czabania"
  },
  "license": "MIT",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --run --coverage",
    "tidy": "biome check --write --unsafe",
    "tidy:check": "biome check",
    "knip": "knip",
    "watch": "node --no-warnings --experimental-strip-types --env-file=.env --watch ./src/index.ts",
    "start": "node --no-warnings --experimental-strip-types --env-file=.env ./src/index.ts",
    "tsc": "tsc"
  },
  "dependencies": {
    "@slack/bolt": "4.1.0",
    "@stayradiated/error-boundary": "4.3.0",
    "arctic": "2.2.1",
    "better-sqlite3": "11.5.0",
    "dotenv": "16.4.5",
    "kysely": "0.27.4",
    "memoize": "10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/better-sqlite3": "7.6.11",
    "@vitest/coverage-v8": "2.1.4",
    "knip": "5.36.2",
    "typescript": "5.6.3",
    "undici": "6.20.1",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.4"
  },
  "engines": {
    "node": "^22.9.0"
  }
}
