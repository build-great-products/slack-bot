{
  "name": "@roughapp/slack-bot",
  "private": true,
  "version": "0.17.0",
  "packageManager": "pnpm@9.15.3",
  "type": "module",
  "main": "./src/index.ts",
  "author": {
    "name": "George Czabania"
  },
  "license": "MIT",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --run --coverage",
    "tidy": "biome check --write --unsafe",
    "tidy:check": "biome check",
    "knip": "knip",
    "watch": "node --no-warnings --experimental-strip-types --env-file=.env --watch ./src/index.ts",
    "start": "node --no-warnings --experimental-strip-types --env-file=.env ./src/index.ts",
    "tsc": "tsc",
    "check": "tsc"
  },
  "dependencies": {
    "@roughapp/sdk": "2.2.0",
    "@slack/bolt": "4.2.0",
    "@stayradiated/error-boundary": "4.3.0",
    "arctic": "3.1.0",
    "better-sqlite3": "11.7.2",
    "dotenv": "16.4.7",
    "kysely": "0.27.5",
    "memoize": "10.0.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/better-sqlite3": "7.6.12",
    "@vitest/coverage-v8": "2.1.8",
    "knip": "5.41.1",
    "typescript": "5.7.2",
    "undici": "7.2.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "engines": {
    "node": "^23.5.0"
  }
}
