{
  "name": "@roughapp/slack-bot",
  "private": true,
  "version": "0.25.0",
  "packageManager": "pnpm@10.11.0",
  "type": "module",
  "main": "./src/index.ts",
  "author": {
    "name": "George Czabania"
  },
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "dependencies": {
    "@roughapp/sdk": "4.4.0",
    "@slack/bolt": "4.6.0",
    "@stayradiated/error-boundary": "4.3.0",
    "dotenv": "17.2.3",
    "graphile-migrate": "1.4.1",
    "kysely": "0.28.10",
    "memoize": "10.2.0",
    "pg": "8.17.1",
    "streaming-iterables": "8.0.1",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/pg": "8.16.0",
    "@vitest/coverage-v8": "4.0.17",
    "kanel": "3.16.0",
    "kanel-kysely": "0.8.0",
    "knip": "5.82.0",
    "test-fixture-factory": "2.1.0",
    "typescript": "5.9.3",
    "undici": "7.18.2",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "4.0.17"
  },
  "knip": {
    "ignore": [
      "src/**/*.test.ts",
      "src/utils/log-query.ts",
      "src/migrations/*.ts",
      "src/__generated__/**"
    ],
    "ignoreDependencies": [
      "graphile-migrate",
      "kanel",
      "kanel-kysely",
      "@biomejs/biome"
    ],
    "ignoreFiles": [
      ".kanelrc.cjs",
      "gmrc.cjs"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
